{
  "name": "timidity",
  "description": "Play MIDI files in the browser w/ Web Audio, WebAssembly, and libtimidity",
  "version": "1.2.0",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "https://feross.org"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "bugs": {
    "url": "https://github.com/feross/timidity/issues"
  },
  "dependencies": {
    "brfs": "^2.0.2",
    "debug": "^4.1.1",
    "freepats": "^1.0.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "standard": "*",
    "tinyify": "^2.5.2"
  },
  "homepage": "https://bitmidi.com",
  "keywords": [
    "api",
    "browser",
    "libtimidity",
    "mid file",
    "midi",
    "midi api",
    "midi file",
    "midi player",
    "player",
    "soundfont",
    "timidity",
    "web audio",
    "webassembly"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/timidity.git"
  },
  "scripts": {
    "build": "./tools/build.sh",
    "download": "./tools/download.sh",
    "size": "npm run build && browserify --plugin tinyify . | gzip | wc -c && cat libtimidity.wasm | gzip | wc -c",
    "test": "standard"
  },
  "standard": {
    "ignore": [
      "libtimidity.js"
    ]
  }
}
